{
    "name": "decodelabs/archetype",
    "description": "Simple named library loader",
    "type": "library",
    "keywords": [ "library", "loader", "finder" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",

        "decodelabs/exceptional": "^0.4",
        "decodelabs/glitch-support": "^0.4",
        "decodelabs/veneer": "^0.11.6"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Archetype\\": "src/"
        },
        "files": [
            "src/Handler.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
