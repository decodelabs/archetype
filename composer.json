{
    "name": "decodelabs/archetype",
    "description": "Simple named library loader",
    "type": "library",
    "keywords": ["library", "loader", "finder"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",

        "decodelabs/exceptional": "^0.4",
        "decodelabs/glitch-support": "^0.4",
        "decodelabs/veneer": "^0.10.10"
    },
    "require-dev": {
        "decodelabs/glitch": "^0.18.5",
        "decodelabs/phpstan-decodelabs": "^0.6.1"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Archetype\\": "src/Archetype"
        },
        "files": [
            "src/global.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    }
}
